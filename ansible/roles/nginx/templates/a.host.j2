location ^~ /a {
  proxy_set_header  X-Real-IP  $remote_addr:{{ external_port }};
  proxy_set_header  X-Forwarded-For $proxy_add_x_forwarded_for:{{ external_port }};
  proxy_set_header  Host $host:{{ external_port }};
  proxy_redirect off;
  proxy_read_timeout 300;
  proxy_pass http://10.0.2.2:8081;
}
